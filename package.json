{
  "name": "react-news-spa",
  "version": "1.0.0",
  "description": "SPA News Website",
  "engines": {
    "node": "10.14.2"
  },
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm test"
    }
  },
  "scripts": {
    "dev": "webpack-dev-server --config ./webpack.client.js --mode development",
    "start": "node server.js",
    "build:prod": "webpack -p --config webpack.client.prod.js --mode production",
    "heroku-postbuild": "npm install && npm run build:prod",
    "webpack-prod": "webpack -p --progress --config webpack.client.prod.js --mode production",
    "dev:build-client": "webpack --config webpack.client.js --watch"
  },
  "author": "ilker ALTIN",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "axios": "0.19.0",
    "compression": "^1.7.4",
    "concurrently": "4.1.0",
    "express": "4.17.1",
    "express-http-proxy": "1.5.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "1.19.3",
    "lodash": "4.17.15",
    "npm-run-all": "4.1.5",
    "prop-types": "^15.7.2",
    "react": "16.8.2",
    "react-dom": "16.10.1",
    "react-lazy-load": "^3.0.13",
    "react-lazy-load-image-component": "^1.3.2",
    "react-redux": "6.0.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.3.1",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "serialize-javascript": "2.1.1",
    "webpack": "4.41.0",
    "webpack-merge": "4.2.1",
    "webpack-node-externals": "1.7.2",
    "compression-webpack-plugin": "^2.0.0",
    "clean-webpack-plugin": "^1.0.1",
    "uglifyjs-webpack-plugin": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.7",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react-hooks": "^2.1.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "react-hot-loader": "^4.7.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "3.8.2"
  }
